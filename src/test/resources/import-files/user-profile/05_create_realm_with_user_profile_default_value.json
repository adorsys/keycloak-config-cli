{
  "enabled": true,
  "realm": "realmWithProfileDefaultValue",
  "attributes": {
    "userProfileEnabled": true
  },
  "userProfile": {
    "attributes": [
      {
        "name": "username",
        "displayName": "${username}",
        "validations": {
          "length": {
            "min": 1,
            "max": 20
          },
          "username-prohibited-characters": {}
        }
      },
      {
        "name": "newsletter",
        "displayName": "${profile.attributes.newsletter}",
        "defaultValue": "false",
        "validations": {
          "options": {
            "options": [
              "true",
              "false"
            ]
          }
        },
        "annotations": {
          "inputType": "select-radiobuttons"
        },
        "permissions": {
          "view": [
            "admin",
            "user"
          ],
          "edit": [
            "admin",
            "user"
          ]
        },
        "multivalued": false
      }
    ]
  }
}
